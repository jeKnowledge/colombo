<h1>Listing Books</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>first_name</th>
      <th>last_name</th>
      <th>rating</th>
      <th>qualifications</th>
      <th>CV</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @user.username %></td>
      <td><%= @user.email %></td>
      <td><%= @user.first_name %></td>
      <td><%= @user.last_name %></td>
      <td><%= @user.rating %></td>
      <td><%= @user.qualifications %></td>
      <td><%= link_to "CV", @user.cv.url %></td>
    </tr>
  </tbody>
</table>

<br>
