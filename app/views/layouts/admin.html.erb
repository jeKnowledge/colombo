    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body id='<%= "#{controller_name}_#{action_name}" %>'>
    <%= render partial: '/admins/shared/sidebar' %>

    <%= yield %>

    <!-- /.container-fluid -->
    <footer class="footer text-center"> 2018 &copy; Colombo </footer>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
